{
  "name": "react-quick-start",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-star-ratings": "^2.3.0",
    "@types/styled-components": "^5.1.25",
    "amazon-chime-sdk-component-library-react": "^3.0.0",
    "amazon-chime-sdk-js": "^3.1.0",
    "bootstrap": "^5.1.3",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-datepicker": "^4.5.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-icons": "^4.4.0",
    "react-localization": "^1.0.17",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^5.0.0",
    "react-star-ratings": "^2.3.0",
    "react-transition-group": "^4.4.2",
    "reactstrap": "^9.0.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.0",
    "sass-loader": "^12.3.0",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/assets/styles -o ./src/assets/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./src/assets/styles -o src/assets/styles/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && build-js",
    "lint:ts": "tslint 'src/**/*.{ts,tsx,js}'",
    "fix": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-datepicker": "^4.3.3"
  }
}
